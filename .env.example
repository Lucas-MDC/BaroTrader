# PostgreSQL Connection Configuration
# Copy this file to .env and fill in the values

# Host and port for PostgreSQL server
HOST=localhost
PORT=5432

# Admin connection (for database/user/role setup)
DB_ADMIN_DBNAME=postgres
DB_ADMIN_USER=postgres
DB_ADMIN_PASS=your_admin_password

# Application database connection
DB_DBNAME=barotrader_db
DB_USER=db_user_barotrader_dev
DB_PASS=your_user_password
